func add_tile(tile, color, r, c):
	var mesh = tile.get_child(0)
	var mat = mesh.get_surface_material(0).duplicate()
	mat.albedo_color = color
	mesh.set_surface_material(0, mat)
	tile.global_translate(Vector3(r, 0, c))
	add_child(tile)

func _ready():
