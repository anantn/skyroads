				tile.global_translate(Vector3(r, 0, c))
				add_child(tile)
				tile = null
			if "t" in col[0]:
				tile = get_tunnel(level["palette"])
				tile.global_translate(Vector3(r, 0, c))
				add_child(tile)
				c = c - 6
